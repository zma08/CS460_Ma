@model IEnumerable<FinalEx.Models.ArtWork>

@{
    ViewBag.Title = "ShowArtWorks";
}

<h2>ShowArtWorks</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Artist.BirthCity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ArtistName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Artist.BirthCity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ArtistName)
        </td>
        
    </tr>
}

</table>
