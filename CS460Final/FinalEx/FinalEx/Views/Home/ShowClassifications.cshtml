@model IEnumerable<FinalEx.Models.Classification>

@{
    ViewBag.Title = "ShowClassifications";
}

<h2>ShowClassifications</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ArtWork.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Genre.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ArtWorkName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GenreName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ArtWork.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Genre.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ArtWorkName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GenreName)
        </td>
        
    </tr>
}

</table>
